# Add Single Item 
$path = Get-Item . | ForEach-Object { $_.ProviderPath } 
$source = Get-Item . | New-ExplicitItemSource -Name "$path" -InstallMode Overwrite -MergeMode Merge
$package.Sources.Add($source);
Close-Window
